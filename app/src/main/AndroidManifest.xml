<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.asoloveva.netcalc">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name="ru.asoloveva.netcalc.CalculatorApp"
        android:allowBackup="false"
        android:icon="@mipmap/ic_calculator"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="ru.asoloveva.netcalc.MainActivity"
            android:windowSoftInputMode="stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="655f4f8b99a7e5834f8852e4e5d5b495ef508134" />
    </application>
</manifest>